{
  "name": "cefiti",
  "version": "5.2.25",
  "description": "Catálogo de Exigências Fitossanitárias para o Trânsito Interestadual",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cefiti/cefiti.git"
  },
  "bugs": {
    "url": "https://github.com/cefiti/cefiti/issues"
  },
  "keywords": [],
  "authors": [
    "Dalci de Jesus Bagolin",
    "Omar Roberto da Silveira"
  ],
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "zip": "jszip ./build -o ./build/CEFiTI.zip",
    "deploy": "jszip -c package.json -o CEFiTI%npm_package_version%.zip",
    "analyze": "source-map-explorer build/static/js/*",
    "lint": "eslint --ignore-pattern ./assests/**/* --ext .ts,.tsx ."
  },
  "dependencies": {
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/d3-array": "^1.2.6",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "d3-array": "^2.0.3",
    "jszip-cli": "^1.4.24",
    "react-scripts": "next",
    "source-map-explorer": "^1.8.0",
    "typescript": "^3.3.3333"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "/assests/**/*",
    "assets/**/*"
  ],
  "entities": [
    {
      "name": "./build",
      "root": true
    },
    "./build/CEFiTI.zip"
  ]
}
