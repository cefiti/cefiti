{
  "name": "cefiti",
  "version": "5.1.24",
  "description": "Catálogo de Exigências Fitossanitárias para o Trânsito Interestadual",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cefiti/cefiti.git"
  },
  "keywords": [],
  "authors": [
    "Dalci de Jesus Bagolin",
    "Omar Roberto da Silveira"
  ],
  "license": "MIT",
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/cefiti/cefiti/issues"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "zip": "jszip ./build -o ./build/CEFiTI.zip",
    "deploy": "jszip -c package.json -o CEFiTI.zip",
    "analyze": "source-map-explorer build/static/js/*",
    "lint": "tslint -p tsconfig.json \"src/**/*.ts?(x)\"",
    "eslint": "eslint --ext .ts,.tsx ."
  },
  "dependencies": {
    "js-plus": "^0.3.0",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "customize-cra": "^0.2.12",
    "eslint-config-prettier": "^4.1.0",
    "jest": "^23.6.0",
    "jszip-cli": "^1.4.24",
    "react-app-rewired": "^2.1.0",
    "react-scripts": "2.1.4",
    "source-map-explorer": "^1.7.0",
    "typescript": "^3.3.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "@typescript-eslint/explicit-member-accessibility": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-type-alias": "off",
      "@typescript-eslint/array-type": "off",
      "react/jsx-uses-vars": "error",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/prefer-interface": "off"
    },
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "eslintIgnore": [
    "/assets/**/*"
  ],
  "entities": [
    {
      "name": "./build",
      "root": true
    },
    "./build/CEFiTI.zip"
  ]
}
