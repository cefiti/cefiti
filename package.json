{
  "name": "cefiti",
  "version": "5.2.27",
  "description": "Catálogo de Exigências Fitossanitárias para o Trânsito Interestadual",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cefiti/cefiti.git"
  },
  "bugs": {
    "url": "https://github.com/cefiti/cefiti/issues"
  },
  "keywords": [],
  "authors": [
    "Dalci de Jesus Bagolin",
    "Omar Roberto da Silveira"
  ],
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "node dev.mjs",
    "testonly": "node --test-reporter spec --test-only --test ./**/*test.cjs ",
    "runtest": "node --test-reporter spec --test ./**/*test.cjs ",   "buildtest": "esbuild --bundle --platform=node  --format=cjs ./src/store.test.ts > ./src/store.test.cjs",
    "test": "npm run buildtest && npm run runtest",
    "analyze": "source-map-explorer public/*.js",
    "lint": "eslint --ignore-pattern ./assests/**/* --ext .ts,.tsx .",
    "build": "esbuild src/index.tsx --bundle --minify --sourcemap --outfile=public/index.js --format=esm --jsx=automatic"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "valtio": "^1.11.2"
  },
  "devDependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "d3-array": "^2.3.2",
    "esbuild": "^0.19.2",
    "source-map-explorer": "^2.1.0",
    "tsx": "^3.12.7",
    "typescript": "^3.6.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "/assests/**/*",
    "assets/**/*"
  ],
  "entities": [
    {
      "name": "./build",
      "root": true
    },
    "./build/CEFiTI.zip"
  ]
}
