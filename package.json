{
  "name": "cefiti",
  "version": "5.2.27",
  "description": "Catálogo de Exigências Fitossanitárias para o Trânsito Interestadual",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cefiti/cefiti.git"
  },
  "bugs": {
    "url": "https://github.com/cefiti/cefiti/issues"
  },
  "keywords": [],
  "authors": [
    "Dalci de Jesus Bagolin",
    "Omar Roberto da Silveira"
  ],
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "zip": "jszip ./build -o ./build/CEFiTI.zip",
    "deploy": "jszip -c package.json -o CEFiTI%npm_package_version%.zip",
    "analyze": "source-map-explorer public/*.js",
    "lint": "eslint --ignore-pattern ./assests/**/* --ext .ts,.tsx .",
    "esbuild": "esbuild src/index.tsx --bundle --minify --sourcemap --outfile=public/bundle.js"
  },
  "dependencies": {
    "mobx": "^5.14.0",
    "mobx-react-lite": "^1.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.07.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "d3-array": "^2.3.2",
    "esbuild": "^0.19.2",
    "jszip-cli": "^1.4.24",
    "source-map-explorer": "^2.1.0",
    "typescript": "^3.6.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "/assests/**/*",
    "assets/**/*"
  ],
  "entities": [
    {
      "name": "./build",
      "root": true
    },
    "./build/CEFiTI.zip"
  ]
}
