{
  "name": "cefiti",
  "version": "3.6.12",
  "description": "Catálogo de Exigências Fitossanitárias para o Trânstio Interestadual",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "test": "mocha -r mocha-clean -r ts-node/register -r dist/db.js test/**/*.* src -w --watch-extensions ts,tsx -R min",
    "ilink": "npm link react react-dom mobx mobx-react mocha mocha-clean ts-node ts-loader source-map-loader"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cefiti/cefiti.git"
  },
  "keywords": [],
  "author": "Dalci de Jesus Bagolin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cefiti/cefiti/issues"
  },
  "homepage": "https://github.com/cefiti/cefiti#readme",
  "jest": {
    "transform": {".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"},
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js" ],
    "rootDir": "app",
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react-native",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "strict": true,
        "noEmitHelpers": true,
        "importHelpers": true
      }
    }
  }
}
