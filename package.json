{
  "version": "4.2.0",
  "description": "Catálogo de Exigências Fitossanitárias para o Trânstio Interestadual",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cefiti/cefiti.git"
  },
  "keywords": [],
  "authors": [
    "Dalci de Jesus Bagolin",
    "Omar Roberto da Silveira"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cefiti/cefiti/issues"
  },
  "dependencies": {
    "detect-browser": "^3.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "js-plus": "^0.1.7",
    "mobx": "^4.5.2",
    "mobx-react": "^5.3.6",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript-eslint-parser": "^20.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@types/jest": "^23.3.8",
    "@types/node": "^10.12.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "customize-cra": "^0.2.0",
    "jest": "^23.6.0",
    "jszip-cli": "^1.4.24",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "2.1.0",
    "ts-jest": "^23.1.4",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.4"
  },
  "scripts": {
    "startp": "parcel ./src/_.html --open",
    "start": "react-app-rewired start",
    "buildp": "parcel build ./src/index.tsx -d ./public",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "testp": "jest test --watchAll",
    "zip": "jszip ./public -o ./public/CEFiTI.zip"
  },
  "jestx": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "extends": "react-app",
    "plugins": [
      "typescript"
    ],
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off",
      "jsx-a11y/anchor-is-valid": "off"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "legacyDecotators": true
      }
    }
  }
}
